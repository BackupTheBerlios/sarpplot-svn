system "rm run/* 2> /dev/null";
system "rm conf/* 2> /dev/null";
system "rm images/* 2> /dev/null";

print "Generating run files...\n";
cpu();
disk();
diskpart();
icmpg();
icmppt();
interr();
ipe ();
ipg ();
keres ();
mempage();
memswa();
nete();
netg();
nfsrpc();
nfsg();
nfserr();
nfsserv();
proc();
tcpe();
tcpg();
