	    $title=~s/.run//;
	    open $element,">$element";
	    print $element "set terminal png size 1024,768;\n";
	    print $element "set xlabel \"time\";\n";
	    print $element "set ylabel \"$title\";\n";
	    print $element "set label \"$title\";\n";
